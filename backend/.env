# ==================================================================
# DWG TO DXF CONVERSION SETTINGS
# ==================================================================

# Primary converter: ODA (more reliable for newer AutoCAD versions)
CONVERTER=oda
CONVERTER_BIN="C:\\Program Files\\ODA\\ODAFileConverter 26.9.0\\ODAFileConverter.exe"

# Backup converter: LibreDWG (used if ODA fails)
#LIBREDWG_BIN=dwg2dxf
LIBREDWG_BIN=C:\LibreDWG\dwg2dxf.exe
# Target DXF version
DXF_VERSION=ACAD2018

# ==================================================================
# REDIS CONFIGURATION
# ==================================================================
REDIS_URL=redis://localhost:6380/0
RESULT_BACKEND=redis://localhost:6380/1

# ==================================================================
# CELERY CONFIGURATION
# ==================================================================
CELERY_BROKER_URL=redis://localhost:6380/0
CELERY_RESULT_BACKEND=redis://localhost:6380/1

# ==================================================================
# PROCESSING SETTINGS
# ==================================================================
MAX_FILE_SIZE_MB=100
PROCESSING_TIMEOUT_SEC=600
LOG_LEVEL=INFO