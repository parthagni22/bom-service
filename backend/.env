# ==================================================================
# DWG TO DXF CONVERSION SETTINGS
# ==================================================================

# Primary converter: ODA FileConverter
CONVERTER=oda

# IMPORTANT: Full path including version folder
CONVERTER_BIN=C:/Program Files/ODA/ODAFileConverter 26.9.0/ODAFileConverter.exe

# Backup converter: LibreDWG
LIBREDWG_BIN=dwg2dxf

# Target DXF version
DXF_VERSION=ACAD2018

# ==================================================================
# REDIS CONFIGURATION
# ==================================================================
REDIS_URL=redis://localhost:6379/0
RESULT_BACKEND=redis://localhost:6379/1

# ==================================================================
# CELERY CONFIGURATION
# ==================================================================
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1

# ==================================================================
# PROCESSING SETTINGS
# ==================================================================
MAX_FILE_SIZE_MB=100
PROCESSING_TIMEOUT_SEC=600
LOG_LEVEL=INFO